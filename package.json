{
  "name": "molecule",
  "version": "0.2.1",
  "description": "Modern starter kit for React, Redux & Electron Apps.",
  "author": "Zach Sherman <zach@timber.io>",
  "license": "MIT",
  "main": "main.js",
  "homepage": "./",
  "scripts": {
    "serve:src": "cross-env NODE_ENV=development babel-node ./tools/srcServer.js",
    "serve:dist": "cross-env NODE_ENV=production babel-node ./tools/distServer.js",
    "clean:dist": "rimraf ./dist",
    "clean:packages:": "rimraf ./packages",
    "lint": "eslint ./src ./tools ./webpackConfig \"./*.js\"",
    "test:watch": "npm run test -- --watch",
    "test": "jest",
    "predev": "babel-node ./tools/startMessage.js",
    "start": "cross-env NODE_ENV=development npm run build:dll && run-p serve:src",
    "prebuild": "run-s clean:dist",
    "build:dll": "babel-node ./tools/dll.js",
    "build": "cross-env NODE_ENV=production babel-node ./tools/build.js",
    "electron": "cross-env NODE_ENV=development electron .",
    "electron-production": "cross-env NODE_ENV=production electron .",
    "prepackage": "run-s clean:packages",
    "package": "electron-packager ./ --out=./packages --icon=./dist/molecule.icns --ignore=\"/node_modules|src|tools|docs|webpackConfig|lock/\"",
    "component": "generact"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "next",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-saga": "^0.15.3",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "styled-components": "^2.1.2"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.0.1",
    "autoprefixer": "^7.1.0",
    "babel-cli": "^6.24.1",
    "babel-core": "6.23.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.24.1",
    "browser-sync": "2.18.8",
    "chalk": "1.1.3",
    "circular-dependency-plugin": "^3.0.0",
    "connect-history-api-fallback": "1.3.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.1",
    "dotenv": "^4.0.0",
    "electron": "^1.6.7",
    "electron-devtools-installer": "^2.2.0",
    "electron-packager": "^8.7.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.0",
    "eslint-import-resolver-webpack": "0.8.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "^0.11.1",
    "fs-extra": "^3.0.1",
    "generact": "^0.1.0",
    "html-webpack-plugin": "2.28.0",
    "imagemin-webpack-plugin": "1.4.4",
    "jest-cli": "^20.0.3",
    "jsdom": "^10.1.0",
    "lost": "8.0.0",
    "npm-run-all": "4.0.2",
    "postcss-loader": "^2.0.5",
    "react-addons-test-utils": "^15.5.1",
    "react-dev-utils": "^0.5.2",
    "react-test-renderer": "^15.5.4",
    "rimraf": "2.6.1",
    "style-loader": "^0.17.0",
    "svg-url-loader": "2.0.2",
    "url-loader": "0.5.8",
    "webpack": "^2.5.1",
    "webpack-bundle-analyzer": "^2.8.1",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-middleware": "1.10.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.18.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/timberio/molecule.git"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testPathIgnorePatterns": [
      "/node_modules",
      "packages"
    ],
    "testRegex": "tests/.*\\.test\\.js$"
  }
}
